{
  "name": "@fabiospampinato/tnt",
  "repository": "github:frog-o/template",
  "description": "A super-simple way to create new projects based on templates and edit there config.",
  "version": "2.0.0",
  "type": "module",
  "bin": "dist/bin.js",
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "types": "dist/index.d.ts",
  "stackblitz": {
    "installDependencies": false,
    "startCommand": "chmod +x ./SBHack.js;./SBHack.js"
  },
  "note": {
    "Compileing": "Tnt supports builing with ebuild \n > $ yarn run build and useing tsc  \n $ yarn run compile"
  },
  "scripts": {
    "clean": "tsex clean",
    "build": "tsup ./src/bin.ts  --format esm --dts --sourcemap --clean  ",
    "compile": "tsex compile",
    "compile:watch": "tsex compile --watch",
    "test": "tsex test",
    "test:watch": "tsex test --watch",
    "prepublishOnly": "tsex prepare",
    "start-old": "node  --loader tsx --inspect ./src/bin.ts",
    "start": "tsx --no-cache ./src/bin.ts"
  },
  "keywords": [
    "template",
    "scaffold",
    "handlebars",
    "picolate",
    "cli",
    "simple"
  ],
  "dependencies": {
    "json-archive": "^2.1.0",
    "lodash": "^4.17.21",
    "picolate": "^1.0.2",
    "prompts": "^2.4.2",
    "radix64-encoding": "^2.0.1",
    "specialist": "^1.2.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.4.2",
    "@types/prompts": "^2.4.4",
    "fava": "^0.2.0",
    "tsex": "^2.2.4",
    "typescript": "^5.0",
    "tsup": "^7.2.0",
    "tsx": "3.12.6"
  }
}
